<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="sanity-testsuite">
	<test name="sanitytest">
		<groups>
			<run>
				<include name="Sanity"></include>
			</run>
		</groups>
		<classes>
			<class
				name="com.supplycopia.bulkupload.dataattribute.CategoryMaster" />
			<class
				name="com.supplycopia.bulkupload.uploaddocument.UploadDocument" />
			<class
				name="com.supplycopia.bulkupload.dataattribute.ItemMasterSanity" />
			<class
				name="com.supplycopia.bulkupload.pricingdatabase.PricingDatabaseSanity" />
			<class
				name="com.supplycopia.bulkupload.mastermanagement.MasterManagement" />
		</classes>

	</test>
	<listeners>
		<!-- <listener class-name="com.supplycopia.listeners.AutoTestListener"></listener> 
			<listener class-name="com.supplycopia.listeners.RetryListener"></listener> -->
		<listener
			class-name="com.supplycopia.report.ExtentReportListener"></listener>
		<listener
			class-name="com.supplycopia.listeners.AnnotationTransformer"></listener>
	</listeners>
</suite>